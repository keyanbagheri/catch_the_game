<body>


    <div class = "center">

      <div id = "navigation">
      <h1><%= @todays_date %></h1>
      <div>

      <div class = "gameinfo">
      
          <% @todays_broadcasts.each do |broadcast| %>
          
          <h2><%= broadcast.title %></h2>

          <h3><%=broadcast.time%></h3>

          <button class = "likebutton" value = "broadcast" data-text-swap="Unlike">Like</button>


   
          <%broadcast.events.each do |event|%>

          <h5><a href="#" title=<%=event.venue.name%><%=event.blurb%>>EVENTS</a>
    
            <% end %>
  
              
            <div class = "line"></div>
          
          <% end %>
        
        

      </div>

    </div>

</body> 