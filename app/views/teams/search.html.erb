<body>
  <h1> Results </h1>
  <%@results.each do |team|%>
<h1><%=team.market + " " + team.name%></h1>
<div>
  <sidebar>
    <img src="<%=team.photo_url%>" width="250" height="250"/>
    <p>Name: <%= team.name%></p>
    <p>City: <%=team.market%></p>
    <p>League: <%=team.league%></p>
    <br>
  </sidebar>
  <article>
    <h3>Broadcasts:</h3>
    <% team.broadcasts.each do |broadcast| %>
    <h1><%= broadcast.visitor_team.name + " vs. " + broadcast.home_team.name %></h1>
<div>
  <sidebar>
    <p>Date: <%=broadcast.datetime%></p>
    <p>Visitor: <%=broadcast.visitor_team.name%></p>
    <p>Home: <%=broadcast.home_team.name%></p>
    <br>
  </sidebar>
  <article>
    <%broadcast.events.each do |event|%>
    <p>Title: <%=event.name%></p>
    <p>Location: <%=event.venue.name%></p>
    <p>Date: <%=event.broadcast.datetime%></p>
    <p>Blurb: <%=event.blurb%></p>
    <p>Info: <%=event.info%></p>
    <br>
    <% end %>
  </article>
</div>
  </article>
</div>
<%end%>
<% end %>
</body> 