<h2>New Event</h2>


<h4>Event Registration List</h4>
<div class='event-manager'>
  <ul class='events'>
  </ul>

  <form class='event-form'>
    <input type="text" name='name' placeholder="name">
    <input type="text" name='blurb' placeholder="blurb">
    <input type="text" name='info' placeholder="info">
    <input type="submit">
  </form>
</div>


<script id="eventTemplate" type="text/template">
  <ul id=<%%= id %> class="event">
  <form class="edit-form">
    <li><h1><%%= name %></h1>
    <input type="hidden" name="name" value=<%%= name %>>
    <button class="edit">Edit</button></li>
    <li>blurb: <%%= blurb %>
    <input type="hidden" name="blurb" value=<%%= blurb %>>
    <button class="edit">Edit</button></li>
    <li>info: <%%= info %>
    <input type="hidden" name="info" value=<%%= info %>>
    <button class="edit">Edit</button></li>
  </form>
  </ul>
</script>